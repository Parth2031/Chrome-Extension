{
  "name": "Modify New Tab'S",
  "version": "1.0.0",
  "description": "Make your own Personalized Tab",
  "manifest_version": 2,
  "icons": {"128": "icon_128.png"},
  "chrome_url_overrides": {
    "newtab": "Extension.html"
  },
  "browser_action": {
    "default_popup": "Popup.html",
    "default_icon": "icon.png"
  },
  "permissions": ["tabs"]
}